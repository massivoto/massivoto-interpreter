{
  "name": "@massivoto/interpreter",
  "version": "0.1.0",
  "description": "Massivoto Interpreter - OTO program execution engine",
  "type": "module",
  "license": "BSL-1.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run clean && tsc --build",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run build",
    "test": "npm run build && vitest run",
    "prettier": "prettier --write \"src/**/*.ts\" \"./*.json\""
  },
  "peerDependencies": {
    "@massivoto/kit": "*",
    "@massivoto/runtime": "*"
  },
  "dependencies": {
    "@masala/parser": "^2.0.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "lodash.set": "^4.3.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/lodash.set": "^4.3.9",
    "@types/node": "^22.10.5",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.7.2",
    "vite-node": "^3.1.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massivoto/massivoto-interpreter.git"
  },
  "author": "Massivoto",
  "keywords": [
    "massivoto",
    "interpreter",
    "oto",
    "automation",
    "dsl"
  ]
}
